  "private": true,
  "scripts": {
    "start": "DEV_PORT=8000 webpack-dev-server --progress --config webpack.config.dev.js",
    "build": "npm run clean:build && NODE_ENV=production webpack -p --config webpack.config.prod.js",
    "start:mock": "NODE_ENV=development GIT_API_URL=http://localhost:3000 webpack-dev-server --progress --config webpack.config.dev.js",
    "build:mock": "npm run clean:build && NODE_ENV=production GIT_API_URL=http://localhost:3000 webpack -p --config webpack.config.prod.js",
